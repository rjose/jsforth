
#-------------------------------------------------------------------------------
# "help" is the default target
#-------------------------------------------------------------------------------
default: help

#-------------------------------------------------------------------------------
# Prints help message
#-------------------------------------------------------------------------------
.PHONY: help
help:
	@echo -e "jsforth Targets"

	@echo -e "\nPROGRAMS"
	@echo -e "\trun:\t\tRuns API server"

.PHONY: run
run:
	python2 api_server.py
